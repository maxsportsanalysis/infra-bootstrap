#!ipxe
echo "Starting iPXE boot"

dhcp

# Adjust to your web server's IP
set base-url http://192.168.50.251

echo "Booting Ubuntu 24.04 Server Autoinstall..."

kernel ${base-url}/pxe/ubuntu/24.04/vmlinuz \
    ip=dhcp \
    url=http://192.168.50.251/pxe/iso/ubuntu-24.04.iso \
    autoinstall \
    ds=nocloud-net;s=${base-url}/autoinstall/ \
    --- autoinstall-verbose debug
initrd ${base-url}/pxe/ubuntu/24.04/initrd
boot